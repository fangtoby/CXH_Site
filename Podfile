source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '8.0'
use_frameworks!
inhibit_all_warnings! #去掉第三方警告
target ‘CXH_Site’ do
    pod 'Alamofire'        #网络请求(swift)
    pod 'Moya' #对Alamofire的封装
    pod 'SDWebImage'
    pod 'ObjectMapper'     #一键json模型互转
    pod 'IQKeyboardManagerSwift'  #管理键盘
    pod 'SnapKit' #自动布局库，swift版
    pod 'Siren'#版本更新
    pod 'SDCycleScrollView' #轮播图
    pod 'SVProgressHUD'  #菊花动画
    pod 'XCGLogger' #日志管理类库
    pod 'Result'
    pod 'SwiftyJSON'
    pod 'JPush'
    pod 'MJRefresh' #下拉刷新
end

